<?xml version="1.0" encoding="UTF-8"?>
<ns1:NCIPMessage xmlns:ns1="http://www.niso.org/2008/ncip"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.niso.org/2008/ncip http://www.niso.org/schemas/ncip/v2_02/ncip_v2_02.xsd"
                 ns1:version="http://www.niso.org/schemas/ncip/v2_02/ncip_v2_02.xsd">
    <!--
    This specifies that the Fiscal Transaction was successfully created, and it provides the ID of the Fiscal Transaction
    for later use.
    The response includes the User ID that the Fiscal Transaction was created for, which should be exactly as it was
    provided in the request since that is what the client understands.
    -->
    <ns1:CreateUserFiscalTransactionResponse>
        <ns1:ResponseHeader>
            <ns1:FromSystemId ns1:Scheme="http://oclc.org/ncip/schemes/systemid/oclcsystemid.scm">WMS Circ</ns1:FromSystemId>
            <ns1:FromAgencyId>
                <ns1:AgencyId ns1:Scheme="http://oclc.org/ncip/schemes/agencyid/symbol.scm">OCWMS</ns1:AgencyId>
            </ns1:FromAgencyId>
            <ns1:ToSystemId ns1:Scheme="http://oclc.org/ncip/schemes/systemid/oclcsystemid.scm">WSILL</ns1:ToSystemId>
            <ns1:ToAgencyId>
                <ns1:AgencyId ns1:Scheme="http://oclc.org/ncip/schemes/agencyid/symbol.scm">OCWMS</ns1:AgencyId>
            </ns1:ToAgencyId>
        </ns1:ResponseHeader>
        <ns1:UserId>
            <ns1:UserIdentifierType ns1:Scheme="http://www.niso.org/ncip/v1_0/imp1/schemes/visibleuseridentifiertype/visibleuseridentifiertype.scm">Barcode</ns1:UserIdentifierType>
            <ns1:UserIdentifierValue>ZYXWV</ns1:UserIdentifierValue>
        </ns1:UserId>
        <ns1:FiscalTransactionReferenceId>
            <ns1:AgencyId ns1:Scheme="http://oclc.org/ncip/schemes/agencyid/symbol.scm">OCWMS</ns1:AgencyId>
            <ns1:FiscalTransactionIdentifierValue>4ddb6543-34d3-43b7-8f07-4c6ca9d38e23</ns1:FiscalTransactionIdentifierValue>
        </ns1:FiscalTransactionReferenceId>
    </ns1:CreateUserFiscalTransactionResponse>
</ns1:NCIPMessage>
